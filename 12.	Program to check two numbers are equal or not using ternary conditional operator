#include <bits/stdc++.h> 
using namespace std; 
    
int main() 
{ 
    cout << "Execute expression using"
    << " ternary operator: ";
    int a = 2 > 5 ? 2 : 5; 
    cout << a << endl; 
        
    cout << "Execute expression using "
    << "if else statement: "; 
        
   
    if ( 2 > 5) 
        cout << "2"; 
    else
        cout << "5"; 
    return 0; 
}
