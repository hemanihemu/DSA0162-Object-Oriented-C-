#include<iostream>
using namespace std;
int main()
{
	int a;
	cout<<"enter a charecter =";
	cin>>a;
    if(a=='A'||a=='Z')
    cout<<"upper case";
    else if(a=='a'||a=='z')
    cout<<"lower case";
    else if(a=='0'||a=='0')
    cout<<"digit";
    else 
    cout<<"special charecter";
	

	
	return 0;
}
